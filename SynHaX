local places = {
    [14735849025] = {tn = "AnM Battlegrounds", url = "https://raw.githubusercontent.com/haxer19/hihihihi/main/game/anm.lua"}
}

function loadScript(url)
    local success, err = pcall(function()
        loadstring(game:HttpGet(url, true))()
    end)
    
    if not success then
        warn("Error: " .. err)
        
        local gui = Instance.new("ScreenGui")
        gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui") 

        local label = Instance.new("TextLabel", gui)
        label.Size = UDim2.new(0, 320, 0, 160)
        label.Position = UDim2.new(0.5, -160, 0.5, -80)
        label.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        label.TextColor3 = Color3.fromRGB(255, 75, 75)
        label.Font = Enum.Font.GothamBold
        label.TextSize = 20
        label.TextWrapped = true
        label.Text = "SynHaX\nError: " .. err
    end
end

local placeg = places[game.PlaceId]
if placeg then
    print("Loading: " .. placeg.tn) 
    loadScript(placeg.url) 
else
    game.Players.LocalPlayer:Kick("Unsupported Map")
end
